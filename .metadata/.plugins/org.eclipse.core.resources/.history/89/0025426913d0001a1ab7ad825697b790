package a1_new_preparation;

import java.util.HashMap;

public class CHaracterIntger {
	
	public static void main(String[] args) {
	
	String str ="deeptha123";
	
	remove(str);
}

public static void remove(String s) {
	int len = s.length();
	
	HashMap<Character, Integer> map = new HashMap<>(); 
	for (int i = 0; i < len; i++) { 
		char key = s.charAt(i);
		if(map.containsKey("[0-9]")) {
			map.remove(key);
		}else {
			map.put(key, 1);
		}
			
		}
		
		
	
	
}
}