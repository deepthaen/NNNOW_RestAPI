package a1_new_preparation;

import java.util.Arrays;

public class SortArr {
	public static void main(String[] args) {
		int[] arr = {24, 6,55,22, 5,10 };
		//sorting(arr);
	
	
	
	
		int len = arr.length;
		  int temp;
	
	for(int i= 0; i<= len; i++) {
		for (int j = i+1; j < len; j++) {
			if(arr[i]>arr[j]) {
			temp = arr[i];
			arr[i] = arr[j];
			arr[j] = temp;
			
		}
		}	
	}

		System.out.println(arr[i]);
	}
}
